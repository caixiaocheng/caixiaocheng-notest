<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white">
    <LinearLayout
        android:id="@+id/mTitleImgContainer" 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">    
    </LinearLayout>
    <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <!-- <com.weiwend.fooldelivery.customviews.MyGallery
	        android:id="@+id/mGallery"
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_marginBottom="@dimen/home_gallery_margin_bottom"
	        android:layout_weight="1"
	        android:visibility="visible"/> -->
	    <com.weiwend.fooldelivery.customviews.MyCycleGallery
	        android:id="@+id/mCycleGallery"
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_marginBottom="@dimen/home_gallery_margin_bottom"
	        android:layout_weight="1"
	        android:visibility="visible"/>
        <LinearLayout 
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_marginLeft="@dimen/home_interval_icon_with_icon"
		    android:layout_marginRight="@dimen/home_interval_icon_with_icon"
	        android:layout_weight="1"
	        android:orientation="horizontal">
	        <LinearLayout
	            android:id="@+id/mQueryContainer" 
	            android:layout_width="match_parent"
	            android:layout_height="match_parent"
	            android:layout_marginRight="@dimen/home_interval_icon_with_icon"
	            android:layout_weight="1"
	            android:orientation="vertical"
	            android:background="@drawable/home_bg1"
	            android:gravity="center">
	            <ImageView 
		            android:layout_width="@dimen/home_icon_big_width"
		            android:layout_height="@dimen/home_icon_big_height"
		            android:background="@drawable/home_icon1"/>
	            <TextView 
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:layout_marginTop="@dimen/home_interval_icon_with_text"
	                android:textSize="@dimen/home_icon_text_size"
	                android:textColor="@color/white"
	                android:text="@string/query"/>	  
	        </LinearLayout>
	        <LinearLayout
	            android:id="@+id/mSentContainer"
	            android:layout_width="match_parent"
	            android:layout_height="match_parent"
	            android:layout_weight="1"
	            android:orientation="vertical"
	            android:background="@drawable/home_bg2"
	            android:gravity="center">
	            <ImageView 
		            android:layout_width="@dimen/home_icon_big_width"
		            android:layout_height="@dimen/home_icon_big_height"
		            android:background="@drawable/home_icon2"/>
	            <TextView 
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:layout_marginTop="@dimen/home_interval_icon_with_text"
	                android:textSize="@dimen/home_icon_text_size"
	                android:textColor="@color/white"
	                android:text="@string/sent"/>
	        </LinearLayout>	        
    	</LinearLayout>
	    <LinearLayout 
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:layout_marginTop="@dimen/home_interval_icon_with_icon"
	        android:layout_marginLeft="@dimen/home_interval_icon_with_icon"
		    android:layout_marginRight="@dimen/home_interval_icon_with_icon"
	        android:layout_weight="1"
	        android:orientation="vertical">
	        <LinearLayout 
		        android:layout_width="match_parent"
		        android:layout_height="match_parent"
		        android:layout_weight="1"
		        android:orientation="horizontal">
	            <LinearLayout
		            android:id="@+id/mNoticeCenterContainer"
		            android:layout_width="match_parent"
		            android:layout_height="match_parent"
		            android:layout_marginRight="@dimen/home_interval_icon_with_icon"
		            android:layout_weight="1"
		            android:orientation="vertical"
		            android:background="@drawable/home_bg3"
		            android:gravity="center">
		            <ImageView
			            android:layout_width="@dimen/home_icon_width"
		                android:layout_height="@dimen/home_icon_height"
			            android:background="@drawable/home_icon3"/>
		            <TextView 
		                android:layout_width="wrap_content"
		                android:layout_height="wrap_content"
		                android:layout_marginTop="@dimen/home_interval_icon_with_text"
		                android:textSize="@dimen/home_icon_text_size"
		                android:textColor="@color/white"
		                android:text="@string/notice_center"/>
	          </LinearLayout>
	          <LinearLayout
		            android:id="@+id/mUserCenterContainer"
		            android:layout_width="match_parent"
		            android:layout_height="match_parent"
		            android:layout_weight="1"
		            android:orientation="vertical"
		            android:background="@drawable/home_bg4"
		            android:gravity="center">
		            <ImageView 
			            android:layout_width="@dimen/home_icon_width"
		                android:layout_height="@dimen/home_icon_height"
			            android:background="@drawable/home_icon4"/>
		            <TextView 
		                android:layout_width="wrap_content"
		                android:layout_height="wrap_content"
		                android:layout_marginTop="@dimen/home_interval_icon_with_text"
		                android:textSize="@dimen/home_icon_text_size"
		                android:textColor="@color/white"
		                android:text="@string/user_center"/>
	          </LinearLayout>
	        </LinearLayout>
	        <LinearLayout 
		        android:layout_width="match_parent"
		        android:layout_height="match_parent"
		        android:layout_marginTop="@dimen/home_interval_icon_with_icon"
		        android:layout_marginLeft="@dimen/home_interval_icon_with_icon"
		        android:layout_marginRight="@dimen/home_interval_icon_with_icon"
		        android:layout_marginBottom="@dimen/home_interval_icon_with_icon"
		        android:layout_weight="1"
		        android:orientation="horizontal">
	            <LinearLayout
		            android:id="@+id/mParityContainer"
		            android:layout_width="match_parent"
		            android:layout_height="match_parent"
		            android:layout_marginRight="@dimen/home_interval_icon_with_icon"
		            android:layout_weight="1"
		            android:orientation="vertical"
		            android:background="@drawable/home_bg5"
		            android:gravity="center">
		            <ImageView 
			            android:layout_width="@dimen/home_icon_width"
		                android:layout_height="@dimen/home_icon_height"
			            android:background="@drawable/home_icon5"/>
		            <TextView 
		                android:layout_width="wrap_content"
		                android:layout_height="wrap_content"
		                android:layout_marginTop="@dimen/home_interval_icon_with_text"
		                android:textSize="@dimen/home_icon_text_size"
		                android:textColor="@color/white"
		                android:text="@string/delivery_parity"/>
	          </LinearLayout>
	          <LinearLayout
		            android:id="@+id/mNearbyContainer"
		            android:layout_width="match_parent"
		            android:layout_height="match_parent"
		            android:layout_weight="1"
		            android:orientation="vertical"
		            android:background="@drawable/home_bg6"
		            android:gravity="center">
		            <ImageView
			            android:layout_width="@dimen/home_icon_width"
		                android:layout_height="@dimen/home_icon_height"
			            android:background="@drawable/home_icon6"/>
		            <TextView 
		                android:layout_width="wrap_content"
		                android:layout_height="wrap_content"
		                android:layout_marginTop="@dimen/home_interval_icon_with_text"
		                android:textSize="@dimen/home_icon_text_size"
		                android:textColor="@color/white"
		                android:text="@string/nearby"/>
	          </LinearLayout>
	        </LinearLayout>
    	</LinearLayout>
    </LinearLayout>
</LinearLayout>

